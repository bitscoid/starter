{
  "name": "@bitscoid/starter",
  "private": true,
  "version": "1.0.1",
  "description": "SvelteKit Starter Kit - Sveltekit Prisma Postgres Jwt Tailwind DaisyUI",
  "author": {
    "name": "Nurul Imam",
    "email": "admin@bits.co.id",
    "url": "https://github.com/bitscoid"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitscoid/starter.git"
  },
  "bugs": {
    "url": "https://github.com/bitscoid/starter/issues"
  },
  "homepage": "https://bits.co.id",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "seed": "node prisma/seed.cjs",
    "reset": "rm -rf node_modules && rm -rf .svelte-kit && pnpm install",
    "start": "pnpm install && pnpm prisma generate && pnpm prisma db push && pnpm seed",
    "db:reset": "npx prisma migrate reset --force",
    "prepare": "svelte-kit sync || echo ''",
    "lint": "eslint . --ext .ts,.svelte",
    "format": "prettier --write .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "prisma": {
    "seed": "node prisma/seed.cjs"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prisma": "^6.12.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@tailwindcss/oxide",
      "esbuild",
      "prisma"
    ]
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@tailwindcss/vite": "^4.1.11",
    "bcryptjs": "^3.0.2",
    "cookie": "^1.0.2",
    "daisyui": "^5.0.46",
    "jsonwebtoken": "^9.0.2",
    "lucide-svelte": "^0.525.0",
    "tailwindcss": "^4.1.11"
  }
}